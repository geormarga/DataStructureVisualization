<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.layout.VBox?>
<VBox fx:id="viewContainer" prefWidth="${viewContainer.scene.width}" prefHeight="${viewContainer.scene.height}"
      xmlns:fx="http://javafx.com/fxml" styleClass="button-container"
      fx:controller="Facade.Controllers.QueueArrayController">
    <HBox>
        <TextField styleClass="text-field"></TextField>
        <Button styleClass="action-button">Enqueue</Button>
        <Button styleClass="action-button">Dequeue</Button>
    </HBox>
    <TilePane fx:id="tilePane" prefWidth="${viewContainer.scene.width}"/>
</VBox>