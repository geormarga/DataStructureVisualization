<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.Group?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<?import javafx.collections.FXCollections?>
<?import javafx.scene.control.Label?>

<Group xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2"
       fx:controller="sample.Contollers.QueueArrayController"
       fx:id="parentGroup">
    <VBox styleClass="root" prefWidth="${parentGroup.scene.width}" prefHeight="${parentGroup.scene.height}">
        <HBox fx:id="toolbar" prefWidth="${parentGroup.scene.width}">
            <Region HBox.hgrow="ALWAYS"/>
            <Button fx:id="btnMinimize" styleClass="control-button" text="_"/>
            <Button fx:id="btnMaximize" styleClass="control-button" text="â–¡"/>
            <Button fx:id="btnClose" styleClass="control-button" text="X"/>
        </HBox>
        <VBox fx:id="vboxContainer" prefWidth="${parentGroup.scene.width}" prefHeight="${parentGroup.scene.height}">
            <fx:define>
                <FXCollections fx:id="list" fx:factory="observableArrayList">
                </FXCollections>
            </fx:define>
        </VBox>
    </VBox>
    <stylesheets>
        <URL value="@../Theme.css"/>
    </stylesheets>
</Group>